{
	"name": "app-ts",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run prettier-format"
		}
	},
	"devDependencies": {
		"husky": "^8.0.1",
		"prettier": "^2.7.1",
		"typescript": "^4.6.4",
		"vite": "3.1.0",
		"vitest": "0.24.3"
	},
	"dependencies": {
		"mathjs": "11.3.0"
	}
}
