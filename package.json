		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"sass": "^1.57.1",
		"vitest": "0.24.3",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@vitejs/plugin-react": "^3.0.0"
	},
	"dependencies": {
		"mathjs": "11.3.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-node": "^10.9.1"
	}
}
